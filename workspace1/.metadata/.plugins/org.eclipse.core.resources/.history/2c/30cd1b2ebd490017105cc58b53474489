package seaching;
import java.io.*;
import java.util.*;



public class search1 {
	public static void main(String[] args)
	{
		int a[]= new int[5];
		
	Scanner sc=new Scanner(System.in);
	
	
		for(int i=0;i<a.length;i++)
		{
			a[i]=sc.nextInt();
		}int x=sc.nextInt();
		for(int i=0;i<a.length;i++)
		{
		if (a[i]==x)
		{
			System.out.println(i);
		}
		}
	}
}
